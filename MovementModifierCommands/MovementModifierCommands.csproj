	</ItemGroup>

	<ItemGroup>
	  <ProjectReference Include="..\MovementModifier - Rocket\MovementModifier - Rocket.csproj" />
	</ItemGroup>

	<ItemGroup>
	  <Reference Include="Rocket.API">
	    <HintPath>..\MovementModifier - Rocket\libs\Rocket.API.dll</HintPath>
	  </Reference>
	  <Reference Include="Rocket.Core">
	    <HintPath>..\MovementModifier - Rocket\libs\Rocket.Core.dll</HintPath>
	  </Reference>
	  <Reference Include="Rocket.Unturned">
	    <HintPath>..\MovementModifier - Rocket\libs\Rocket.Unturned.dll</HintPath>
	  </Reference>
